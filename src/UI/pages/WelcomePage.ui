   <string notr="true">/* GLOBAL BACKGROUND */
QWidget#WelcomePage {
	background-color: #FAFAFA; /* Very light subtle grey/white */
}

/* HEADER SECTION */
QWidget#headerContainer {
    /* Modern Gradient: Deep Blue to Purple */
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 #1CB5E0, stop:1 #000046);
    border-bottom-left-radius: 30px;
    border-bottom-right-radius: 30px;
}

QLabel#lblHeaderTitle {
	color: white;
	font-family: 'Segoe UI', sans-serif;
	font-size: 32px;
	font-weight: bold;
}

QLabel#lblHeaderSub {
	color: #E0E0E0;
	font-family: 'Segoe UI', sans-serif;
	font-size: 16px;
}

/* WELCOME TEXT */
QLabel#label_4 {
    color: #2c3e50;
    font-size: 28px;
    font-weight: bold;
}

QLabel#label_5 {
    color: #555555;
    font-size: 14px;
    margin-bottom: 20px;
}

/* ACTION BUTTONS (Start, About, Team) */
QPushButton {
	border: 1px solid #E0E0E0;
	border-radius: 12px;
	padding: 15px;
	text-align: left;
    background-color: white;
    font-family: 'Segoe UI', sans-serif;
}

QPushButton:hover {
	background-color: #F0F8FF; /* Light Alice Blue */
    border: 1px solid #B0C4DE;
    transform: translateY(-2px); /* Note: Qt CSS doesn't support transform animations usually, but nice to have in web */
}

/* Custom Colors for Buttons */
QPushButton#btnStart {
    border-left: 5px solid #1CB5E0; /* Blue accent */
}
QPushButton#btnAbout {
    border-left: 5px solid #FFD700; /* Gold accent */
}
QPushButton#btnTeam {
    border-left: 5px solid #FF6B6B; /* Red/Pink accent */
}

/* FEATURE CIRCLES */
/* We will style these individually in the widget properties for unique colors */
QLabel {
    font-family: 'Segoe UI', sans-serif;
}
